{
  "name": "server",
  "version": "1.0.0",
  "description": "Local server for vr-analytics",
  "main": "index.js",
  "scripts": {
    "start-mongo": "mongod --dbpath C:/data",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/index.js"
  },
  "keywords": [
    "vr-analytics"
  ],
  "author": "Mihai Pop",
  "license": "ISC",
  "dependencies": {
    "express": "^4.15.2",
    "mongodb": "^2.2.31"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "body-parser": "^1.17.2",
    "express": "^4.15.2",
    "rimraf": "^2.6.1"
  }
}
